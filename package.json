{
  "name": "dataset_web",
  "version": "1.3.0",
  "description": "",
  "main": "electron-main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/index.js",
    "electron:dev": "concurrently \"npm run client:dev\" \"electron .\"",
    "client:dev": "npm run dev --prefix client",
    "electron:build": "npx npm-run-all --serial build:client build:electron",
    "build:client": "npm run build --prefix client",
    "build:electron": "npx electron-builder"
  },
  "build": {
    "appId": "com.pose-annotator.app",
    "productName": "Pose Annotator",
    "directories": {
      "output": "dist"
    },
    "files": [
      "electron-main.js",
      "server.js",
      "package.json",
      "node_modules/**/*",
      "client/dist/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Pose Annotator"
    },
    "asar": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "archiver": "^7.0.1",
    "body-parser": "^2.2.2",
    "core-util-is": "^1.0.3",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "image-size": "^2.0.2",
    "inherits": "^2.0.4",
    "isarray": "^2.0.5",
    "multer": "^2.0.2",
    "sharp": "^0.34.5",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^40.3.0",
    "electron-builder": "^26.7.0",
    "npm-run-all": "^4.1.5",
    "wait-on": "^9.0.3"
  }
}